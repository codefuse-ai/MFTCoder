{
    "load_raw_dataset": true,
    "data_paths": "[/path/to/dataset1,/path/to/dataset2]",
    "output_dir": "/path/to/store/fine-tuned/model",
    "tb_dir": "/path/to/store/tensorboard/log",
    "pretrained_model_path": "/path/to/Qwen-1_8B/model",
    "vocab_file": "/path/to/Qwen-1_8B/model",
    "low_cpu_mem_usage": true,
    "data_split": "95,5,0",
    "padding_mode": "padding",
    "tokenize_mode": "sft",
    "weighted_loss_mode": "case3",
    "model_type": "qwen",
    "peft_type": "lora",
    "quantization": "16bit",
    "lora_rank": 96,
    "lora_alpha": 32,
    "lora_dropout": 0.05,
    "per_device_train_batch_size": 8,
    "per_device_eval_batch_size": 8,
    "tokenizer_type": "AutoTokenizer",
    "learning_rate": 5e-5,
    "min_lr": 1e-6,
    "weight_decay": 0.1,
    "gradient_accumulation_steps": 1,
    "lr_scheduler_type": "cosine",
    "num_warmup_steps": 300,
    "num_train_epochs": 10,
    "seed": 1234,
    "seq_length": 4096,
    "resume_from_checkpoint": null,
    "log_interval": 100,
    "checkpointing_steps": 1000,
    "evalation_steps": 1000,
    "max_train_steps": null,
    "epoch_checkpointing": true,
    "shuffle_before_split": true,
    "use_random_sampler": true,
    "early_stopping": true,
    "early_stopping_stall_num": 2,
    "weight_by_num_documents": true,
    "make_vocab_size_divisible_by": 128,
    "model_parallel_size": 1,
    "use_slow_tokenizer": false,
    "use_xformers": true,
    "trust_remote_code": true,
    "use_dynamic_padding": true,
    "world_size": 8
}